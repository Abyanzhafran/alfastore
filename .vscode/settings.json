  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "conventionalCommits.scopes": [
    "package"
  ],
}